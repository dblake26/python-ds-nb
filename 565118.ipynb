   "source": [
    "# Define the number of rows for the pattern\n",
    "num_rows = 4\n",
    "\n",
    "# Loop through each row\n",
    "for i in range(1, num_rows + 1):\n",
    "    # Print '*' repeated i times, followed by ' ' repeated num_rows - i times\n",
    "    print('*' * i + ' ' * (num_rows - i) + '*' * i)\n"
   ]
